<h1><%= title %></h1>

<table border=1>
  <tr>
    <th>id</th>
    <th>name</th>
    <th>delete</th>
  </tr>
  <form action="/delete" method="POST">
  <% for (var i in users) { %>
  <tr>
    <td><a href="/update/<%= users[i].id %>"><%= users[i].seq %></a></td>
    <td><%= users[i].name %></td>
    <td align="center"><input type="radio" name="id" value="<%= users[i].id %>" /></td>
  </tr>
  <% } %>
</table>
<input type="submit" value="選択した項目を削除">
</form>

<br />
<a href="/create">新規追加</a><br />
